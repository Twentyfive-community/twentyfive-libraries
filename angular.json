{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Twentyfive-libraries": {
      "projectType": "library",
      "root": "projects/twentyfive-libraries",
      "sourceRoot": "projects/twentyfive-libraries/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/twentyfive-libraries/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/twentyfive-libraries/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/twentyfive-libraries/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/twentyfive-libraries/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }

  },
  "cli": {
    "analytics": "826c6298-079d-4571-b9ae-6aba585a3255"
  }
}
