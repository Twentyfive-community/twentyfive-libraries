{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/twentyfive-style",
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": ["node_modules"]
  },
  "assets": [
    "./src/assets/**/*.*"
  ],

  "allowedNonPeerDependencies": [
    "@popperjs/core",
     "@ng-bootstrap/ng-bootstrap",
    "@popperjs/core",
    "bootstrap",
    "bootstrap-icons",
    "sass-loader",
    "resolve-url-loader",
    "css-loader"
  ]
}
